{
  "name": "void",
  "version": "1.0.0",
  "description": "A reimplementation of the twitch backend made using typescript",
  "main": "dist/src/index.js",
  "scripts": {
    "setup": "npm install && npm run build && npm start",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "prisma-migrate": "npx prisma migrate dev --name init",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skiesuwu/Void.git"
  },
  "keywords": [
    "Twitch",
    "Void",
    "Twitch.tv",
    "twitch",
    "void.tv"
  ],
  "author": "Skiesuwu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Skiesuwu/Void/issues"
  },
  "homepage": "https://github.com/Skiesuwu/Void#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "eslint": "^8.43.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "prettier": "^2.8.8",
    "prisma": "^4.16.1",
    "sqlite3": "^5.1.6",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.16.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ts-node-dev": "^2.0.0"
  }
}
